<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:id="@+id/open_rl_container"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:id="@+id/open_header_view"
        android:background="#16182357"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:textSize="14sp"
            android:textColor="#161823"
            android:id="@+id/cancel"
            android:paddingLeft="16dp"
            android:paddingRight="16dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/aweme_open_ssl_cancel"
            android:layout_centerVertical="true"
            android:paddingStart="16dp"
            android:paddingEnd="16dp"/>
        <ImageView
            android:gravity="center"
            android:id="@+id/tiktok_logo"
            android:background="@drawable/tiktok_logo"
            android:layout_width="18dp"
            android:layout_height="20.25dp"
            android:layout_marginTop="15.88dp"
            android:layout_marginBottom="15.88dp"
            android:layout_centerInParent="true"
            android:layout_marginVertical="15.88dp"/>
    </RelativeLayout>
    <FrameLayout
        android:id="@+id/open_loading_group"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/open_header_view"/>
</RelativeLayout>
