<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:tag="layout/layout_challenge_comment_delete_report_0"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:id="@+id/report"
        android:background="@drawable/ripple_effect_transparent_rectangle"
        android:paddingTop="14dp"
        android:paddingBottom="14dp"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/general.report"
        android:paddingStart="24dp"
        android:paddingEnd="24dp"
        app:layout_constraintEnd_toEndOf="0"
        app:layout_constraintStart_toStartOf="0"
        app:layout_constraintTop_toTopOf="0"/>
    <TextView
        android:id="@+id/delete"
        android:background="@drawable/ripple_effect_transparent_rectangle"
        android:paddingTop="14dp"
        android:paddingBottom="14dp"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/general.delete"
        android:paddingStart="24dp"
        android:paddingEnd="24dp"
        app:layout_constraintEnd_toEndOf="0"
        app:layout_constraintStart_toStartOf="0"
        app:layout_constraintTop_toBottomOf="@+id/report"/>
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/appCompatImageView"
        android:layout_width="0dp"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="24dp"
        app:layout_constraintBottom_toBottomOf="@+id/report"
        app:layout_constraintEnd_toEndOf="0"
        app:layout_constraintStart_toStartOf="0"
        app:layout_constraintTop_toBottomOf="@+id/report"
        style="@style/Divider"/>
</androidx.constraintlayout.widget.ConstraintLayout>
