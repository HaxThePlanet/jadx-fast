<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:tag="layout/layout_dashboard_timer_0"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <include
        android:id="@+id/days_segment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="0"
        app:layout_constraintTop_toTopOf="0"
        layout="@layout/layout_dashboard_timer_square_segment"/>
    <include
        android:id="@+id/hour_segment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        app:layout_constraintStart_toEndOf="@+id/days_segment"
        app:layout_constraintTop_toTopOf="0"
        layout="@layout/layout_dashboard_timer_square_segment"/>
    <include
        android:id="@+id/minute_segment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        app:layout_constraintStart_toEndOf="@+id/hour_segment"
        app:layout_constraintTop_toTopOf="0"
        layout="@layout/layout_dashboard_timer_square_segment"/>
    <include
        android:id="@+id/second_segment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        app:layout_constraintStart_toEndOf="@+id/minute_segment"
        app:layout_constraintTop_toTopOf="0"
        layout="@layout/layout_dashboard_timer_square_segment"/>
</androidx.constraintlayout.widget.ConstraintLayout>
