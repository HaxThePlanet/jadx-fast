<?xml version="1.0" encoding="utf-8"?>
<app.dogo.com.dogo_android.util.customview.shimmer.ShimmerFrameLayout xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/shimmer_view"
    android:tag="layout/layout_dashboard_discount_counter_0"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:shimmer_base_alpha="1"
    app:shimmer_direction="1"
    app:shimmer_dropoff="3"
    app:shimmer_duration="1500"
    app:shimmer_highlight_alpha="0.5"
    app:shimmer_repeat_delay="5000"
    app:shimmer_tilt="70">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/parent_view"
        android:tag="binding_1"
        android:background="@drawable/special_offer_default_gold"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/discount_text"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/margin_4"
            android:layout_marginBottom="@dimen/margin_4"
            android:layout_marginStart="@dimen/margin_24"
            android:layout_marginEnd="8dp"
            app:layout_constraintBottom_toBottomOf="0"
            app:layout_constraintEnd_toStartOf="@+id/timer_layout"
            app:layout_constraintStart_toStartOf="0"
            app:layout_constraintTop_toTopOf="0"
            style="@style/DashboardTryDogoPremiumDiscountOff"/>
        <include
            android:id="@+id/timer_layout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:layout_marginBottom="12dp"
            android:layout_marginEnd="@dimen/margin_24"
            app:layout_constraintBottom_toBottomOf="0"
            app:layout_constraintEnd_toEndOf="0"
            app:layout_constraintTop_toTopOf="0"
            layout="@layout/layout_dashboard_timer"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</app.dogo.com.dogo_android.util.customview.shimmer.ShimmerFrameLayout>
