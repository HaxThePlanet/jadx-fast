<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:tag="layout/cell_exam_history_0"
    android:paddingBottom="28dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <ImageView
        android:id="@+id/exam_image"
        android:tag="binding_1"
        android:layout_width="@dimen/lesson_card_image_size"
        android:layout_height="@dimen/lesson_card_image_size"
        android:layout_marginTop="28dp"
        android:layout_marginStart="4dp"
        app:layout_constraintStart_toStartOf="0"
        app:layout_constraintTop_toTopOf="0"/>
    <ImageView
        android:id="@+id/play_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:src="@drawable/ic_play_button"
        app:layout_constraintBottom_toBottomOf="@+id/exam_image"
        app:layout_constraintEnd_toEndOf="@+id/exam_image"
        app:layout_constraintStart_toStartOf="@+id/exam_image"
        app:layout_constraintTop_toTopOf="@+id/exam_image"/>
    <TextView
        android:textColor="#ff6b65"
        android:id="@+id/exam_status"
        android:tag="binding_2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@+id/exam_image"
        app:layout_constraintTop_toTopOf="@+id/exam_image"
        style="@style/TrainingProgramExamListStatusBold"/>
    <TextView
        android:textColor="#4c5d56"
        android:id="@+id/exam_name"
        android:tag="binding_3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toStartOf="@+id/date"
        app:layout_constraintStart_toStartOf="@+id/exam_status"
        app:layout_constraintTop_toBottomOf="@+id/exam_status"
        style="@style/TrainingProgramExamListName"/>
    <TextView
        android:id="@+id/date"
        android:tag="binding_4"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/exam_image"
        app:layout_constraintEnd_toEndOf="0"
        app:layout_constraintTop_toTopOf="@+id/exam_image"
        style="@style/ExamHistoryFeedbackTitle"/>
    <androidx.cardview.widget.CardView
        android:id="@+id/feedback_card"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginStart="4dp"
        android:layout_marginEnd="4dp"
        app:cardCornerRadius="10dp"
        app:cardElevation="1dp"
        app:layout_constraintStart_toStartOf="0"
        app:layout_constraintTop_toBottomOf="@+id/exam_image">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <ImageView
                android:id="@+id/feedback_image"
                android:tag="binding_5"
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:layout_marginTop="38dp"
                android:layout_marginStart="24dp"
                app:layout_constraintStart_toStartOf="0"
                app:layout_constraintTop_toTopOf="0"/>
            <TextView
                android:id="@+id/title"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/exam.nps.dog.trainer"
                android:layout_marginStart="12dp"
                android:layout_marginEnd="24dp"
                app:layout_constraintEnd_toEndOf="0"
                app:layout_constraintStart_toEndOf="@+id/feedback_image"
                app:layout_constraintTop_toTopOf="@+id/feedback_image"
                style="@style/ExamHistoryFeedbackTitle"/>
            <TextView
                android:id="@+id/trainer_name"
                android:tag="binding_6"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:layout_marginStart="12dp"
                android:layout_marginEnd="24dp"
                app:layout_constraintEnd_toEndOf="0"
                app:layout_constraintStart_toEndOf="@+id/feedback_image"
                app:layout_constraintTop_toBottomOf="@+id/title"
                style="@style/ExamHistoryTrainerName"/>
            <TextView
                android:id="@+id/trainer_feedback_text"
                android:tag="binding_7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="28dp"
                android:layout_marginStart="24dp"
                android:layout_marginEnd="24dp"
                app:layout_constraintStart_toStartOf="0"
                app:layout_constraintTop_toBottomOf="@+id/barrier"
                style="@style/ExamHistoryFeedbackText"/>
            <Button
                android:textSize="13sp"
                android:id="@+id/retake_video_button"
                android:tag="binding_8"
                android:layout_width="0dp"
                android:layout_height="36dp"
                android:layout_marginTop="18dp"
                android:text="@string/retake.video"
                android:layout_marginStart="24dp"
                android:layout_marginEnd="24dp"
                android:elevation="0dp"
                app:layout_constraintEnd_toEndOf="0"
                app:layout_constraintStart_toStartOf="0"
                app:layout_constraintTop_toBottomOf="@+id/trainer_feedback_text"
                style="@style/OnboardingYellowButton"/>
            <ImageView
                android:id="@+id/divider"
                android:layout_marginTop="29dp"
                app:layout_constraintStart_toStartOf="0"
                app:layout_constraintTop_toBottomOf="@+id/retake_video_button"
                style="@style/RowDividerSlim"/>
            <TextView
                android:gravity="top"
                android:id="@+id/help_us"
                android:tag="binding_9"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:text="@string/exam.nps.help.us.improve"
                android:layout_marginStart="24dp"
                android:layout_marginEnd="24dp"
                app:layout_constraintEnd_toEndOf="0"
                app:layout_constraintStart_toStartOf="0"
                app:layout_constraintTop_toBottomOf="@+id/divider"
                style="@style/ExamHistoryFeedbackText"/>
            <TextView
                android:textColor="#00b970"
                android:gravity="top"
                android:id="@+id/leave_feedback"
                android:tag="binding_10"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="16dp"
                android:text="@string/exam.nps.help.us.improve.button"
                android:fontFamily="sans-serif-medium"
                app:layout_constraintBottom_toBottomOf="0"
                app:layout_constraintEnd_toEndOf="@+id/help_us"
                app:layout_constraintHorizontal_bias="0"
                app:layout_constraintStart_toStartOf="@+id/help_us"
                app:layout_constraintTop_toBottomOf="@+id/help_us"
                style="@style/ExamHistoryFeedbackText"/>
            <View
                android:id="@+id/leave_feedback_click_area"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintBottom_toBottomOf="@+id/leave_feedback"
                app:layout_constraintEnd_toEndOf="@+id/help_us"
                app:layout_constraintStart_toStartOf="@+id/help_us"
                app:layout_constraintTop_toTopOf="@+id/help_us"/>
            <androidx.constraintlayout.widget.Barrier
                android:id="@+id/barrier"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:barrierDirection="3"
                app:constraint_referenced_ids="trainer_name,feedback_image"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>
