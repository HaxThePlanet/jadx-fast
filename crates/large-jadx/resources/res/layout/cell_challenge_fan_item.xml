<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:android="http://schemas.android.com/apk/res/android"
    android:tag="layout/cell_challenge_fan_item_0"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <ImageView
        app:layout_constraintBottom_toBottomOf="0"
        app:layout_constraintEnd_toEndOf="0"
        app:layout_constraintStart_toStartOf="0"
        app:layout_constraintTop_toTopOf="0"
        app:layout_constraintVertical_bias="1"
        style="@style/RowDivider"/>
    <View
        android:id="@+id/ambassador_badge_anchor"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="4dp"
        android:layout_marginStart="4dp"
        app:layout_constraintStart_toEndOf="@+id/profile_icon"
        app:layout_constraintTop_toBottomOf="@+id/profile_icon"/>
    <ImageView
        android:id="@+id/profile_icon"
        android:tag="binding_1"
        android:layout_width="@dimen/user_profile_icon"
        android:layout_height="@dimen/user_profile_icon"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="16dp"
        android:layout_marginStart="16dp"
        app:layout_constraintBottom_toBottomOf="0"
        app:layout_constraintEnd_toEndOf="0"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintStart_toStartOf="0"
        app:layout_constraintTop_toTopOf="0"
        app:layout_constraintVertical_bias="0"
        app:srcCompat="@drawable/profile_placeholder"/>
    <ImageView
        android:id="@+id/ambassador_badge"
        android:tag="binding_2"
        android:layout_width="15dp"
        android:layout_height="18dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        app:layout_constraintBottom_toBottomOf="@+id/ambassador_badge_anchor"
        app:layout_constraintEnd_toEndOf="@+id/ambassador_badge_anchor"
        app:srcCompat="@drawable/ambassador_badge_vector"/>
    <ImageView
        android:id="@+id/premium_badge"
        android:tag="binding_3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:adjustViewBounds="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="4dp"
        android:layout_marginEnd="4dp"
        app:layout_constraintBottom_toBottomOf="@+id/profile_icon"
        app:layout_constraintBottom_toTopOf="@+id/ambassador_badge_anchor"
        app:layout_constraintEnd_toEndOf="@+id/profile_icon"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintStart_toStartOf="@+id/profile_icon"
        app:layout_constraintTop_toBottomOf="@+id/profile_icon"
        app:srcCompat="@drawable/premium_badge_with_star"/>
    <ImageView
        android:id="@+id/admin_badge"
        android:tag="binding_4"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:adjustViewBounds="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="4dp"
        android:layout_marginEnd="4dp"
        app:layout_constraintBottom_toBottomOf="@+id/profile_icon"
        app:layout_constraintBottom_toTopOf="@+id/ambassador_badge_anchor"
        app:layout_constraintEnd_toEndOf="@+id/profile_icon"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintStart_toStartOf="@+id/profile_icon"
        app:layout_constraintTop_toBottomOf="@+id/profile_icon"
        app:srcCompat="@drawable/badge_admin"/>
    <TextView
        android:id="@+id/title"
        android:tag="binding_5"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/profile_icon"
        app:layout_constraintEnd_toEndOf="0"
        app:layout_constraintStart_toEndOf="@+id/profile_icon"
        app:layout_constraintTop_toTopOf="@+id/profile_icon"/>
    <ImageView
        android:id="@+id/loading_spinner"
        android:tag="binding_6"
        android:background="@color/lightestGray"
        android:visibility="gone"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="0"
        app:layout_constraintEnd_toEndOf="0"
        app:layout_constraintStart_toStartOf="0"
        app:layout_constraintTop_toTopOf="0"/>
</androidx.constraintlayout.widget.ConstraintLayout>
