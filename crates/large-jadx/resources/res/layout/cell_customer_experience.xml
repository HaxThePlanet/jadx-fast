<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:tag="layout/cell_customer_experience_0"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="24dp"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="24dp"
        app:cardCornerRadius="12dp"
        app:cardElevation="4dp"
        app:layout_constraintBottom_toBottomOf="0"
        app:layout_constraintEnd_toEndOf="0"
        app:layout_constraintStart_toStartOf="0"
        app:layout_constraintTop_toTopOf="0"
        app:layout_constraintVertical_bias="0">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/name"
                android:tag="binding_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                app:layout_constraintEnd_toEndOf="0"
                app:layout_constraintStart_toStartOf="0"
                app:layout_constraintTop_toTopOf="0"
                style="@style/CustomerExperienceTitle"/>
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/image"
                android:tag="binding_2"
                android:layout_width="140dp"
                android:layout_height="140dp"
                android:layout_marginTop="10dp"
                app:layout_constraintEnd_toEndOf="0"
                app:layout_constraintStart_toStartOf="0"
                app:layout_constraintTop_toBottomOf="@+id/name"/>
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/star1"
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:layout_marginTop="12dp"
                android:src="@drawable/ic_star"
                android:layout_marginStart="24dp"
                app:layout_constraintEnd_toStartOf="@+id/star2"
                app:layout_constraintStart_toStartOf="@+id/image"
                app:layout_constraintTop_toBottomOf="@+id/image"/>
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/star2"
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:layout_marginTop="12dp"
                android:src="@drawable/ic_star"
                app:layout_constraintEnd_toStartOf="@+id/star3"
                app:layout_constraintStart_toEndOf="@+id/star1"
                app:layout_constraintTop_toBottomOf="@+id/image"/>
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/star3"
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:layout_marginTop="12dp"
                android:src="@drawable/ic_star"
                app:layout_constraintEnd_toStartOf="@+id/star4"
                app:layout_constraintStart_toEndOf="@+id/star2"
                app:layout_constraintTop_toBottomOf="@+id/image"/>
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/star4"
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:layout_marginTop="12dp"
                android:src="@drawable/ic_star"
                app:layout_constraintEnd_toStartOf="@+id/star5"
                app:layout_constraintStart_toEndOf="@+id/star3"
                app:layout_constraintTop_toBottomOf="@+id/image"/>
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/star5"
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:layout_marginTop="12dp"
                android:src="@drawable/ic_star"
                android:layout_marginEnd="24dp"
                app:layout_constraintEnd_toEndOf="@+id/image"
                app:layout_constraintStart_toEndOf="@+id/star4"
                app:layout_constraintTop_toBottomOf="@+id/image"/>
            <TextView
                android:id="@+id/text"
                android:tag="binding_3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="12dp"
                android:layout_marginBottom="24dp"
                android:layout_marginStart="24dp"
                android:layout_marginEnd="24dp"
                app:layout_constraintBottom_toBottomOf="0"
                app:layout_constraintEnd_toEndOf="0"
                app:layout_constraintStart_toStartOf="0"
                app:layout_constraintTop_toBottomOf="@+id/star1"
                app:layout_constraintVertical_bias="0"
                style="@style/CustomerExperienceText"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>
