<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/root"
    android:tag="layout/layout_program_progress_bar_0"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <include
        android:id="@+id/module1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:layout_marginEnd="2dp"
        app:layout_constraintEnd_toStartOf="@+id/module2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_chainStyle="1"
        app:layout_constraintStart_toStartOf="0"
        layout="@layout/layout_program_module_progress_bar"/>
    <include
        android:id="@+id/module2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:layout_marginEnd="2dp"
        app:layout_constraintEnd_toStartOf="@+id/module3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/module1"
        layout="@layout/layout_program_module_progress_bar"/>
    <include
        android:id="@+id/module3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:layout_marginEnd="2dp"
        app:layout_constraintEnd_toStartOf="@+id/module4"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/module2"
        layout="@layout/layout_program_module_progress_bar"/>
    <include
        android:id="@+id/module4"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:layout_marginEnd="2dp"
        app:layout_constraintEnd_toEndOf="0"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/module3"
        layout="@layout/layout_program_module_progress_bar"/>
    <ImageView
        android:id="@+id/module_arrow1"
        android:tag="binding_1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginStart="2dp"
        android:layout_marginEnd="2dp"
        app:layout_constraintEnd_toStartOf="@+id/module_arrow2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_chainStyle="1"
        app:layout_constraintStart_toStartOf="0"
        app:layout_constraintTop_toBottomOf="@+id/module1"
        app:srcCompat="@drawable/ic_dashboard_progress_blunt_arrow"/>
    <ImageView
        android:id="@+id/module_arrow2"
        android:tag="binding_2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginStart="2dp"
        android:layout_marginEnd="2dp"
        app:layout_constraintBottom_toBottomOf="0"
        app:layout_constraintEnd_toStartOf="@+id/module_arrow3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/module_arrow1"
        app:layout_constraintTop_toBottomOf="@+id/module1"
        app:srcCompat="@drawable/ic_dashboard_progress_blunt_arrow"/>
    <ImageView
        android:id="@+id/module_arrow3"
        android:tag="binding_3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginStart="2dp"
        android:layout_marginEnd="2dp"
        app:layout_constraintBottom_toBottomOf="@+id/module_arrow1"
        app:layout_constraintEnd_toStartOf="@+id/module_arrow4"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/module_arrow2"
        app:layout_constraintTop_toBottomOf="@+id/module1"
        app:srcCompat="@drawable/ic_dashboard_progress_blunt_arrow"/>
    <ImageView
        android:id="@+id/module_arrow4"
        android:tag="binding_4"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginStart="2dp"
        android:layout_marginEnd="2dp"
        app:layout_constraintBottom_toBottomOf="@+id/module_arrow1"
        app:layout_constraintEnd_toEndOf="0"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/module_arrow3"
        app:layout_constraintTop_toBottomOf="@+id/module1"
        app:srcCompat="@drawable/ic_dashboard_progress_blunt_arrow"/>
</androidx.constraintlayout.widget.ConstraintLayout>
