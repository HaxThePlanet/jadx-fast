<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/root"
    android:tag="layout/layout_top_bar_0"
    android:fitsSystemWindows="true"
    android:clipToPadding="false"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <FrameLayout
        android:id="@+id/back_button_touch_area"
        android:tag="binding_1"
        android:background="@drawable/button_ripple_effect"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:minWidth="@dimen/touch_minimum"
        android:minHeight="@dimen/touch_minimum"
        app:layout_constraintBottom_toBottomOf="0"
        app:layout_constraintEnd_toEndOf="0"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintStart_toStartOf="0"
        app:layout_constraintTop_toTopOf="0">
        <androidx.appcompat.widget.AppCompatImageView
            android:layout_gravity="center_vertical"
            android:id="@+id/back_icon"
            android:tag="binding_2"
            android:layout_width="@dimen/tiny_image_size"
            android:layout_height="@dimen/tiny_image_size"
            android:src="@drawable/vector_back_arrow"/>
    </FrameLayout>
    <TextView
        android:textSize="@dimen/normal_text_size"
        android:id="@+id/title_text"
        android:tag="binding_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:lineSpacingExtra="8sp"
        android:fontFamily="sans-serif-medium"
        android:layout_marginStart="16dp"
        android:letterSpacing="0.01"
        app:layout_constraintBottom_toBottomOf="0"
        app:layout_constraintEnd_toEndOf="0"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintStart_toEndOf="@+id/back_button_touch_area"
        app:layout_constraintTop_toTopOf="0"/>
    <FrameLayout
        android:id="@+id/block_action_button_touch_area"
        android:tag="binding_4"
        android:background="@drawable/button_ripple_effect"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:minWidth="@dimen/touch_minimum"
        android:minHeight="@dimen/touch_minimum"
        android:foregroundGravity="0x10"
        android:layout_marginEnd="4dp"
        app:layout_constraintBottom_toBottomOf="0"
        app:layout_constraintEnd_toStartOf="@+id/action_button_touch_area"
        app:layout_constraintHorizontal_bias="1"
        app:layout_constraintStart_toEndOf="@+id/title_text"
        app:layout_constraintTop_toTopOf="0">
        <ImageView
            android:layout_gravity="0x800015"
            android:layout_width="@dimen/tiny_image_size"
            android:layout_height="@dimen/tiny_image_size"
            android:layout_marginEnd="16dp"
            app:srcCompat="@drawable/ic_block"/>
    </FrameLayout>
    <FrameLayout
        android:id="@+id/action_button_touch_area"
        android:tag="binding_5"
        android:background="@drawable/button_ripple_effect"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:minWidth="@dimen/touch_minimum"
        android:minHeight="@dimen/touch_minimum"
        android:foregroundGravity="0x10"
        android:layout_marginEnd="4dp"
        app:layout_constraintBottom_toBottomOf="0"
        app:layout_constraintEnd_toStartOf="@+id/more_action_button_touch_area"
        app:layout_constraintHorizontal_bias="1"
        app:layout_constraintStart_toEndOf="@+id/title_text"
        app:layout_constraintTop_toTopOf="0">
        <TextView
            android:textSize="@dimen/card_small_text"
            android:layout_gravity="0x800015"
            android:id="@+id/action_button_text"
            android:tag="binding_6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:lineSpacingExtra="2sp"
            android:textAllCaps="true"
            android:fontFamily="sans-serif-medium"
            android:textAlignment="textEnd"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="12dp"
            android:letterSpacing="0.09"/>
        <ImageView
            android:layout_gravity="0x800015"
            android:tag="binding_7"
            android:layout_width="@dimen/tiny_image_size"
            android:layout_height="@dimen/tiny_image_size"
            android:layout_marginEnd="16dp"
            app:srcCompat="@drawable/ic_outlined_flag"/>
    </FrameLayout>
    <FrameLayout
        android:id="@+id/more_action_button_touch_area"
        android:tag="binding_8"
        android:background="@drawable/button_ripple_effect"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:minWidth="@dimen/touch_minimum"
        android:minHeight="@dimen/touch_minimum"
        android:foregroundGravity="0x10"
        app:layout_constraintBottom_toBottomOf="0"
        app:layout_constraintEnd_toEndOf="0"
        app:layout_constraintHorizontal_bias="1"
        app:layout_constraintStart_toEndOf="@+id/title_text"
        app:layout_constraintTop_toTopOf="0">
        <ImageView
            android:layout_gravity="0x800015"
            android:layout_width="@dimen/tiny_image_size"
            android:layout_height="@dimen/tiny_image_size"
            android:layout_marginEnd="16dp"
            app:srcCompat="@drawable/vector_three_dots"/>
    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
