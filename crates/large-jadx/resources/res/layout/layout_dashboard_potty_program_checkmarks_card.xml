<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.card.MaterialCardView xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/program_card"
    android:tag="layout/layout_dashboard_potty_program_checkmarks_card_0"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:cardCornerRadius="10dp"
    app:cardElevation="1dp"
    app:rippleColor="@color/transparent"
    app:strokeColor="#70e4e4e4"
    app:strokeWidth="1dp">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:background="@drawable/ripple_effect_transparent_rectangle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/title"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/margin_14"
            android:text="@string/potty.tracker.idle.state.header"
            android:layout_marginStart="@dimen/margin_20"
            android:layout_marginEnd="@dimen/margin_20"
            app:layout_constrainedWidth="true"
            app:layout_constraintEnd_toEndOf="0"
            app:layout_constraintStart_toStartOf="0"
            app:layout_constraintTop_toTopOf="0"
            style="@style/PottyCardTitleTextStyle"/>
        <ImageView
            android:id="@+id/divider"
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:layout_marginTop="@dimen/margin_12"
            android:layout_marginStart="@dimen/margin_20"
            android:layout_marginEnd="@dimen/margin_20"
            app:layout_constraintEnd_toEndOf="0"
            app:layout_constraintStart_toStartOf="0"
            app:layout_constraintTop_toBottomOf="@+id/title"
            style="@style/RowDivider"/>
        <Button
            android:id="@+id/calendar_button"
            android:tag="binding_1"
            android:layout_width="wrap_content"
            android:layout_height="32dp"
            android:layout_marginTop="@dimen/margin_12"
            android:layout_marginBottom="@dimen/margin_16"
            android:text="@string/potty.schedule.header.70"
            android:layout_marginStart="@dimen/margin_20"
            android:layout_marginEnd="@dimen/margin_20"
            app:layout_constraintBottom_toBottomOf="0"
            app:layout_constraintEnd_toEndOf="0"
            app:layout_constraintHorizontal_bias="0"
            app:layout_constraintStart_toStartOf="0"
            app:layout_constraintTop_toBottomOf="@+id/divider"
            app:layout_constraintVertical_bias="0"
            style="@style/YellowPottyCardButton"/>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:orientation="horizontal"
            android:id="@+id/check_group_view"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:layout_marginStart="@dimen/margin_12"
            android:layout_marginEnd="@dimen/margin_14"
            app:layout_constraintBottom_toBottomOf="@+id/calendar_button"
            app:layout_constraintEnd_toEndOf="0"
            app:layout_constraintStart_toEndOf="@+id/calendar_button"
            app:layout_constraintTop_toTopOf="@+id/calendar_button">
            <CheckBox
                android:id="@+id/toggle_button1"
                android:tag="binding_2"
                android:layout_width="38dp"
                android:layout_height="32dp"
                app:layout_constraintBottom_toBottomOf="0"
                app:layout_constraintEnd_toStartOf="@+id/toggle_button2"
                app:layout_constraintStart_toStartOf="0"
                app:layout_constraintTop_toTopOf="0"
                style="@style/PottyTrackerCheckMark"/>
            <CheckBox
                android:id="@+id/toggle_button2"
                android:tag="binding_3"
                android:layout_width="38dp"
                android:layout_height="32dp"
                app:layout_constraintBottom_toBottomOf="0"
                app:layout_constraintEnd_toStartOf="@+id/toggle_button3"
                app:layout_constraintStart_toEndOf="@+id/toggle_button1"
                app:layout_constraintTop_toTopOf="0"
                style="@style/PottyTrackerCheckMark"/>
            <CheckBox
                android:id="@+id/toggle_button3"
                android:tag="binding_4"
                android:layout_width="38dp"
                android:layout_height="32dp"
                app:layout_constraintBottom_toBottomOf="0"
                app:layout_constraintEnd_toStartOf="@+id/toggle_button4"
                app:layout_constraintStart_toEndOf="@+id/toggle_button2"
                app:layout_constraintTop_toTopOf="0"
                style="@style/PottyTrackerCheckMark"/>
            <CheckBox
                android:id="@+id/toggle_button4"
                android:tag="binding_5"
                android:layout_width="38dp"
                android:layout_height="32dp"
                app:layout_constraintBottom_toBottomOf="0"
                app:layout_constraintEnd_toStartOf="@+id/toggle_button5"
                app:layout_constraintStart_toEndOf="@+id/toggle_button3"
                app:layout_constraintTop_toTopOf="0"
                style="@style/PottyTrackerCheckMark"/>
            <CheckBox
                android:id="@+id/toggle_button5"
                android:tag="binding_6"
                android:layout_width="38dp"
                android:layout_height="32dp"
                app:layout_constraintBottom_toBottomOf="0"
                app:layout_constraintEnd_toEndOf="0"
                app:layout_constraintStart_toEndOf="@+id/toggle_button4"
                app:layout_constraintTop_toTopOf="0"
                style="@style/PottyTrackerCheckMark"/>
            <View
                android:id="@+id/checkmark_click_area1"
                android:tag="binding_7"
                android:background="@drawable/ripple_effect_transparent_rectangle"
                android:layout_width="0dp"
                android:layout_height="36dp"
                app:layout_constraintBottom_toBottomOf="@+id/toggle_button1"
                app:layout_constraintEnd_toEndOf="@+id/toggle_button1"
                app:layout_constraintStart_toStartOf="@+id/toggle_button1"
                app:layout_constraintTop_toTopOf="@+id/toggle_button1"/>
            <View
                android:id="@+id/checkmark_click_area2"
                android:tag="binding_8"
                android:background="@drawable/ripple_effect_transparent_rectangle"
                android:layout_width="0dp"
                android:layout_height="36dp"
                app:layout_constraintBottom_toBottomOf="@+id/toggle_button2"
                app:layout_constraintEnd_toEndOf="@+id/toggle_button2"
                app:layout_constraintStart_toStartOf="@+id/toggle_button2"
                app:layout_constraintTop_toTopOf="@+id/toggle_button2"/>
            <View
                android:id="@+id/checkmark_click_area3"
                android:tag="binding_9"
                android:background="@drawable/ripple_effect_transparent_rectangle"
                android:layout_width="0dp"
                android:layout_height="36dp"
                app:layout_constraintBottom_toBottomOf="@+id/toggle_button3"
                app:layout_constraintEnd_toEndOf="@+id/toggle_button3"
                app:layout_constraintStart_toStartOf="@+id/toggle_button3"
                app:layout_constraintTop_toTopOf="@+id/toggle_button3"/>
            <View
                android:id="@+id/checkmark_click_area4"
                android:tag="binding_10"
                android:background="@drawable/ripple_effect_transparent_rectangle"
                android:layout_width="0dp"
                android:layout_height="36dp"
                app:layout_constraintBottom_toBottomOf="@+id/toggle_button4"
                app:layout_constraintEnd_toEndOf="@+id/toggle_button4"
                app:layout_constraintStart_toStartOf="@+id/toggle_button4"
                app:layout_constraintTop_toTopOf="@+id/toggle_button4"/>
            <View
                android:id="@+id/checkmark_click_area5"
                android:tag="binding_11"
                android:background="@drawable/ripple_effect_transparent_rectangle"
                android:layout_width="0dp"
                android:layout_height="36dp"
                app:layout_constraintBottom_toBottomOf="@+id/toggle_button5"
                app:layout_constraintEnd_toEndOf="@+id/toggle_button5"
                app:layout_constraintStart_toStartOf="@+id/toggle_button5"
                app:layout_constraintTop_toTopOf="@+id/toggle_button5"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</com.google.android.material.card.MaterialCardView>
