<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:android="http://schemas.android.com/apk/res/android"
    android:tag="layout/cell_program_overview_exam_locked_grid_item_0"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <ImageView
        android:id="@+id/trick_image"
        android:tag="binding_1"
        android:layout_width="56dp"
        android:layout_height="56dp"
        android:layout_marginTop="12dp"
        app:layout_constraintEnd_toEndOf="0"
        app:layout_constraintStart_toStartOf="0"
        app:layout_constraintTop_toTopOf="0"/>
    <ImageView
        android:id="@+id/lock_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/trick_image"
        app:layout_constraintEnd_toEndOf="@+id/trick_image"
        app:layout_constraintStart_toStartOf="@+id/trick_image"
        app:layout_constraintTop_toTopOf="@+id/trick_image"
        app:srcCompat="@drawable/ic_exam_locked"/>
    <TextView
        android:id="@+id/trick_name"
        android:tag="binding_2"
        android:layout_width="@dimen/program_overview_exam_cell_item_width"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:layout_marginBottom="8dp"
        app:layout_constrainedWidth="true"
        app:layout_constraintBottom_toBottomOf="0"
        app:layout_constraintEnd_toEndOf="0"
        app:layout_constraintStart_toStartOf="0"
        app:layout_constraintTop_toBottomOf="@+id/trick_image"
        style="@style/ProgramOverviewExamCellName"/>
</androidx.constraintlayout.widget.ConstraintLayout>
