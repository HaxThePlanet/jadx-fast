<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:tag="layout/layout_dashboard_combined_potty_program_card_0"
    android:clipChildren="false"
    android:clipToPadding="false"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:id="@+id/section_name"
        android:tag="binding_1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_12"
        app:layout_constraintEnd_toStartOf="@+id/program_click_area"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintStart_toStartOf="0"
        app:layout_constraintTop_toTopOf="0"
        style="@style/ProgramListTitle"/>
    <TextView
        android:textSize="14sp"
        android:textColor="#00b970"
        android:id="@+id/program"
        android:background="@drawable/ripple_effect_transparent_rectangle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/program.header"
        app:layout_constraintBottom_toBottomOf="@+id/section_name"
        app:layout_constraintEnd_toStartOf="@+id/program_arrow"
        app:layout_constraintTop_toTopOf="@+id/section_name"
        style="@style/ProgramLessonFullProgram"/>
    <ImageView
        android:id="@+id/program_arrow"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:adjustViewBounds="true"
        android:contentDescription="@null"
        app:layout_constraintBottom_toBottomOf="@+id/program"
        app:layout_constraintEnd_toEndOf="0"
        app:layout_constraintTop_toTopOf="@+id/program"
        app:layout_constraintVertical_bias="1"
        app:srcCompat="@drawable/arrow_right_green"/>
    <View
        android:id="@+id/program_click_area"
        android:tag="binding_2"
        android:layout_width="0dp"
        android:layout_height="@dimen/touch_minimum"
        app:layout_constraintBottom_toBottomOf="@+id/program"
        app:layout_constraintEnd_toEndOf="0"
        app:layout_constraintHeight_min="@dimen/touch_minimum"
        app:layout_constraintHorizontal_bias="0.51"
        app:layout_constraintStart_toStartOf="@+id/program"
        app:layout_constraintTop_toTopOf="@+id/program"
        app:layout_constraintWidth_min="@dimen/touch_minimum"/>
    <include
        android:id="@+id/program_card"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_20"
        app:layout_constraintEnd_toEndOf="0"
        app:layout_constraintStart_toStartOf="0"
        app:layout_constraintTop_toBottomOf="@+id/section_name"
        layout="@layout/layout_dashboard_potty_program_card"/>
    <include
        android:id="@+id/program_card_v2"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_20"
        app:layout_constraintEnd_toEndOf="0"
        app:layout_constraintStart_toStartOf="0"
        app:layout_constraintTop_toBottomOf="@+id/section_name"
        layout="@layout/layout_dashboard_potty_program_card_v2"/>
    <include
        android:id="@+id/schedule_card"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_20"
        app:layout_constraintEnd_toEndOf="0"
        app:layout_constraintStart_toStartOf="0"
        app:layout_constraintTop_toBottomOf="@+id/section_name"
        layout="@layout/layout_dashboard_schedule_card"/>
    <include
        android:id="@+id/potty_checkmarks_card"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_20"
        app:layout_constraintEnd_toEndOf="0"
        app:layout_constraintStart_toStartOf="0"
        app:layout_constraintTop_toBottomOf="@+id/section_name"
        layout="@layout/layout_dashboard_potty_program_checkmarks_card"/>
</androidx.constraintlayout.widget.ConstraintLayout>
