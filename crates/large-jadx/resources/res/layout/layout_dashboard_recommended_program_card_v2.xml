<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:android="http://schemas.android.com/apk/res/android"
    android:tag="layout/layout_dashboard_recommended_program_card_v2_0"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <androidx.cardview.widget.CardView
        android:id="@+id/program_card"
        android:tag="binding_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardBackgroundColor="@color/white"
        app:cardCornerRadius="12dp"
        app:cardElevation="0dp"
        app:layout_constraintEnd_toEndOf="0"
        app:layout_constraintStart_toStartOf="0"
        app:layout_constraintTop_toTopOf="0">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:background="@drawable/ripple_effect_transparent_rectangle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <ImageView
                android:id="@+id/program_image"
                android:tag="binding_2"
                android:layout_width="wrap_content"
                android:layout_height="145dp"
                android:adjustViewBounds="true"
                app:layout_constraintEnd_toEndOf="0"
                app:layout_constraintHeight_default="2"
                app:layout_constraintTop_toTopOf="0"/>
            <TextView
                android:id="@+id/label"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="@string/based.survey.recommended.text"
                android:paddingStart="10dp"
                app:layout_constraintStart_toStartOf="0"
                app:layout_constraintTop_toTopOf="0"
                style="@style/SurveyLabel"/>
            <TextView
                android:id="@+id/fit"
                android:tag="binding_3"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                app:layout_constraintBottom_toBottomOf="@+id/label"
                app:layout_constraintStart_toEndOf="@+id/label"
                app:layout_constraintTop_toTopOf="@+id/label"
                style="@style/SurveyCompatibilityText"/>
            <androidx.constraintlayout.widget.Guideline
                android:orientation="vertical"
                android:id="@+id/title_guide"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintGuide_begin="230dp"/>
            <TextView
                android:textSize="22dp"
                android:id="@+id/program_name"
                android:tag="binding_4"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="24dp"
                app:layout_constraintBottom_toBottomOf="@+id/divider"
                app:layout_constraintEnd_toStartOf="@+id/title_guide"
                app:layout_constraintStart_toStartOf="0"
                app:layout_constraintTop_toBottomOf="@+id/label"
                style="@style/ProgramCardTextTitle"/>
            <View
                android:id="@+id/divider"
                android:background="@color/white"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                app:layout_constraintBottom_toBottomOf="@+id/program_image"
                app:layout_constraintEnd_toEndOf="0"
                app:layout_constraintStart_toStartOf="0"/>
            <ImageView
                android:id="@+id/left_cloud"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:alpha="0.2"
                app:layout_constraintStart_toStartOf="0"
                app:layout_constraintTop_toBottomOf="@+id/divider"
                app:layout_constraintVertical_bias="0.2"
                app:srcCompat="@drawable/vector_cloud_left"/>
            <TextView
                android:textSize="16dp"
                android:textColor="@color/white"
                android:id="@+id/title"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="@string/recommended.program.dog.learn.header"
                android:layout_marginStart="24dp"
                android:layout_marginEnd="24dp"
                app:layout_constraintEnd_toEndOf="0"
                app:layout_constraintStart_toStartOf="0"
                app:layout_constraintTop_toBottomOf="@+id/divider"
                style="@style/ProgramCardTextTitle"/>
            <ImageView
                android:id="@+id/right_cloud"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:alpha="0.2"
                app:layout_constraintEnd_toEndOf="0"
                app:layout_constraintTop_toBottomOf="@+id/title"
                app:layout_constraintVertical_bias="0.2"
                app:srcCompat="@drawable/vector_cloud_right"/>
            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/learnings"
                android:tag="binding_5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="12dp"
                android:layout_marginStart="24dp"
                android:layout_marginEnd="24dp"
                app:layout_constraintStart_toStartOf="0"
                app:layout_constraintTop_toBottomOf="@+id/title"/>
            <Button
                android:textSize="14sp"
                android:id="@+id/review_program_button"
                android:tag="binding_6"
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="20dp"
                android:text="@string/training.program.review"
                android:layout_marginStart="24dp"
                android:layout_marginEnd="24dp"
                android:elevation="0dp"
                app:layout_constraintEnd_toEndOf="0"
                app:layout_constraintStart_toStartOf="0"
                app:layout_constraintTop_toBottomOf="@+id/learnings"
                style="@style/DashboardWhiteButton"/>
            <ImageView
                android:id="@+id/skills_icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="24dp"
                android:layout_marginBottom="24dp"
                android:adjustViewBounds="true"
                android:layout_marginStart="24dp"
                app:layout_constraintBottom_toBottomOf="0"
                app:layout_constraintEnd_toStartOf="@+id/skills"
                app:layout_constraintStart_toStartOf="0"
                app:layout_constraintTop_toBottomOf="@+id/review_program_button"
                app:layout_constraintVertical_chainStyle="2"
                app:srcCompat="@drawable/ic_dog"
                app:tint="@color/black"/>
            <TextView
                android:textColor="@color/icon_gray"
                android:id="@+id/skills"
                android:tag="binding_7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toBottomOf="0"
                app:layout_constraintEnd_toStartOf="@+id/text_separation_left"
                app:layout_constraintStart_toEndOf="@+id/skills_icon"
                app:layout_constraintTop_toBottomOf="@+id/review_program_button"
                app:layout_constraintVertical_chainStyle="2"
                style="@style/ProgramCardTextDescription"/>
            <View
                android:id="@+id/text_separation_left"
                android:background="@color/icon_gray"
                android:layout_width="1dp"
                android:layout_height="28dp"
                app:layout_constraintBottom_toBottomOf="@+id/skills"
                app:layout_constraintEnd_toStartOf="@+id/exams_icon"
                app:layout_constraintStart_toEndOf="@+id/skills"
                app:layout_constraintTop_toTopOf="@+id/skills"
                app:layout_constraintVertical_chainStyle="2"/>
            <ImageView
                android:id="@+id/exams_icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="24dp"
                android:layout_marginBottom="24dp"
                android:adjustViewBounds="true"
                app:layout_constraintBottom_toBottomOf="0"
                app:layout_constraintEnd_toStartOf="@+id/exams"
                app:layout_constraintStart_toEndOf="@+id/text_separation_left"
                app:layout_constraintTop_toBottomOf="@+id/review_program_button"
                app:layout_constraintVertical_chainStyle="2"
                app:srcCompat="@drawable/ic_whistle"
                app:tint="@color/black"/>
            <TextView
                android:textColor="@color/icon_gray"
                android:id="@+id/exams"
                android:tag="binding_8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintEnd_toStartOf="@+id/text_separation_right"
                app:layout_constraintStart_toEndOf="@+id/exams_icon"
                app:layout_constraintTop_toTopOf="@+id/skills"
                app:layout_constraintVertical_chainStyle="2"
                style="@style/ProgramCardTextDescription"/>
            <View
                android:id="@+id/text_separation_right"
                android:background="@color/icon_gray"
                android:layout_width="1dp"
                android:layout_height="28dp"
                app:layout_constraintBottom_toBottomOf="@+id/skills"
                app:layout_constraintEnd_toStartOf="@+id/enrolled_icon"
                app:layout_constraintStart_toEndOf="@+id/exams"
                app:layout_constraintTop_toTopOf="@+id/skills"
                app:layout_constraintVertical_chainStyle="2"/>
            <ImageView
                android:id="@+id/enrolled_icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="24dp"
                android:layout_marginBottom="24dp"
                android:adjustViewBounds="true"
                app:layout_constraintBottom_toBottomOf="0"
                app:layout_constraintEnd_toStartOf="@+id/enrolled"
                app:layout_constraintStart_toEndOf="@+id/text_separation_right"
                app:layout_constraintTop_toBottomOf="@+id/review_program_button"
                app:layout_constraintVertical_chainStyle="2"
                app:srcCompat="@drawable/ic_group_of_people"
                app:tint="@color/black"/>
            <TextView
                android:textColor="@color/icon_gray"
                android:id="@+id/enrolled"
                android:tag="binding_9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="24dp"
                app:layout_constraintEnd_toEndOf="0"
                app:layout_constraintStart_toEndOf="@+id/enrolled_icon"
                app:layout_constraintTop_toTopOf="@+id/skills"
                app:layout_constraintVertical_chainStyle="2"
                style="@style/ProgramCardTextDescription"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>
