<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:android="http://schemas.android.com/apk/res/android"
    android:tag="layout/dialog_challenge_details_0"
    android:focusable="true"
    android:clickable="true"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.google.android.material.appbar.MaterialToolbar
        android:id="@+id/toolbar"
        android:tag="binding_1"
        android:layout_alignParentTop="true"
        app:layout_constraintStart_toStartOf="0"
        app:layout_constraintTop_toTopOf="0"
        app:navigationIcon="@drawable/ic_back_arrow_gray"
        app:titleTextColor="@color/darkGray"
        style="@style/dailyTrainingToolbarWithLayout"/>
    <ScrollView
        android:id="@+id/scrollView"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="0"
        app:layout_constraintEnd_toEndOf="0"
        app:layout_constraintStart_toStartOf="0"
        app:layout_constraintTop_toBottomOf="@+id/toolbar"
        app:layout_constraintVertical_bias="0">
        <LinearLayout
            android:orientation="vertical"
            android:tag="binding_2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <ImageView
                android:id="@+id/challenge_cover"
                android:tag="binding_3"
                android:layout_width="match_parent"
                android:layout_height="194dp"
                android:layout_marginTop="16dp"
                android:scaleType="fitCenter"/>
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp">
                <TextView
                    android:textSize="@dimen/card_normal_text"
                    android:tag="binding_4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/challenge.this.week"
                    android:textAllCaps="true"
                    android:layout_marginStart="16dp"
                    android:layout_marginEnd="16dp"/>
                <TextView
                    android:textSize="@dimen/title_text_size"
                    android:textStyle="0x1"
                    android:tag="binding_5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/montserrat_bold"
                    android:layout_marginStart="16dp"
                    android:layout_marginEnd="16dp"/>
                <TextView
                    android:textSize="@dimen/card_small_text"
                    android:tag="binding_6"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:layout_marginBottom="16dp"
                    android:layout_marginStart="16dp"
                    android:layout_marginEnd="16dp"/>
                <FrameLayout
                    android:id="@+id/sponsor_banner"
                    android:tag="binding_7"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="28dp"
                    android:layout_marginStart="8dp"
                    android:layout_marginEnd="8dp">
                    <include
                        android:id="@+id/friendly_collar_details_banner"
                        android:layout_width="match_parent"
                        android:layout_height="132dp"
                        layout="@layout/layout_sponsor_friendly_collar_details_banner"/>
                    <include
                        android:id="@+id/barkbox_details_banner"
                        android:layout_width="match_parent"
                        android:layout_height="132dp"
                        layout="@layout/layout_sponsor_barkbox_details_banner"/>
                    <include
                        android:id="@+id/universal_details_banner"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        layout="@layout/layout_sponsor_universal_details_banner"/>
                </FrameLayout>
                <ImageView
                    android:layout_height="1dp"
                    style="@style/RowDivider"/>
            </LinearLayout>
            <include
                android:id="@+id/prize_rules_tab"
                layout="@layout/cell_challenge_details_tab"/>
            <include
                android:id="@+id/rules_tab"
                layout="@layout/cell_challenge_details_tab"/>
            <include
                android:id="@+id/privacy_policy_tab"
                layout="@layout/cell_challenge_details_tab"/>
            <ImageView
                android:layout_height="1dp"
                style="@style/RowDivider"/>
            <RelativeLayout
                android:tag="binding_8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="16dp">
                <CheckBox
                    android:id="@+id/check_mark"
                    android:tag="binding_9"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/challenge.participation.policy"
                    android:layout_marginStart="16dp"
                    android:layout_toEndOf="@+id/check_mark"/>
            </RelativeLayout>
            <ImageView
                android:layout_height="1dp"
                style="@style/RowDivider"/>
            <FrameLayout
                android:tag="binding_10"
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:layout_margin="16dp"/>
        </LinearLayout>
    </ScrollView>
    <FrameLayout
        android:id="@+id/fabas"
        android:tag="binding_11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="0"
        app:layout_constraintEnd_toEndOf="0"
        app:layout_constraintStart_toStartOf="0"
        app:layout_constraintTop_toTopOf="0"
        app:layout_constraintVertical_bias="1">
        <include layout="@layout/layout_custom_fab"/>
    </FrameLayout>
    <FrameLayout
        android:tag="binding_12"
        android:visibility="gone"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:elevation="50dp"
        app:layout_constraintBottom_toBottomOf="0"
        app:layout_constraintEnd_toEndOf="0"
        app:layout_constraintStart_toStartOf="0"
        app:layout_constraintTop_toTopOf="0">
        <include
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            layout="@layout/layout_dog_creation_loading_spinner"/>
    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
